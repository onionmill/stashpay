{
  "name": "StashPay",
  "version": "0.1.6",
  "private": true,
  "scripts": {
    "android": "react-native run-android --list-devices",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "build": "yarn build:clean && yarn build:bump && yarn build:android && yarn build:ios && yarn build:tag",
    "build:clean": "rm -rf build android/app/build android/build",
    "build:bump": "git diff --exit-code && npm version patch --no-git-tag-version && fastlane bump",
    "build:android": "fastlane android build && mkdir -p build && cp android/app/build/outputs/apk/release/app-release.apk build/stashpay-$npm_package_version.apk",
    "build:ios": "fastlane ios build && mkdir -p build && mv StashPay.ipa StashPay.app.dSYM.zip build/",
    "build:tag": "git add package.json ios android && git commit -m $npm_package_version && git tag $npm_package_version && git push && git push --tags"
  },
  "dependencies": {
    "@breeztech/react-native-breez-sdk-liquid": "0.5.1",
    "@react-native-async-storage/async-storage": "^2.1.0",
    "@react-native-clipboard/clipboard": "^1.15.0",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "bip39": "^3.1.0",
    "buffer": "^6.0.3",
    "mobx": "^6.13.5",
    "mobx-react": "^9.2.0",
    "react": "18.3.1",
    "react-native": "0.75.2",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.21.2",
    "react-native-get-random-values": "^1.11.0",
    "react-native-keychain": "^9.2.2",
    "react-native-qrcode-svg": "^6.3.12",
    "react-native-reanimated": "^3.16.6",
    "react-native-safe-area-context": "^4.14.1",
    "react-native-screens": "^3.35.0",
    "react-native-share": "^12.0.3",
    "react-native-svg": "^15.10.1",
    "react-native-vector-icons": "^10.2.0",
    "react-native-vision-camera": "^4.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.75.2",
    "@react-native/eslint-config": "0.75.2",
    "@react-native/metro-config": "0.75.2",
    "@react-native/typescript-config": "0.75.2",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.3.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
